<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="menu_shein_scrapper" model="website.menu">
        <field name="name">Shein2Egypt</field>
        <field name="url">/Shein2egypt</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence" type="int">30</field>
    </record>

    <template id="Shein_page" name="Shein2Egypt">
        <t t-call="website.layout">
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
                  integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
                  crossorigin="anonymous"/>
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"
                    integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
                    crossorigin="anonymous"/>
            <link rel="stylesheet" type="text/css" href="shein2egypt/static/scss/nav.css"/>
            <link rel="stylesheet" type="text/css" href="shein2egypt/static/scss/shein2egypt.css"/>
            <link rel="stylesheet" type="text/css" href="shein2egypt/static/scss/login.css"/>
            <!--fav icon-->
            <link rel="shortcut icon" href="g letter black.png"/>
            <script src="https://kit.fontawesome.com/c8e4d183c2.js" crossorigin="anonymous"></script>
            <script src="js/jquery.js"></script>
            <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Sakkal Majalla"/>
            <link rel="stylesheet"
                  href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"/>


            <form action="/Shein2egypt">
                <div class="shein2egypt" style="max-width:1400px">
                    <h1 class="shein2egypt-text">Shop Any Online SHEIN Store in USA,UK or UAE</h1>
                    <h3 class="shein">Delivers to your address in Egypt,Pay in Egyptian Pounds</h3>
                    <br>
                        <div class="form">
                            <input type="text" class="search-box-shein" id="Url" name="Url"
                                   placeholder="Product link here (FULL URL)"/>
                            <button class="search-btn-shein">search</button>
                        </div>
                    </br>
                    <div class="shein-img">
                        <a href="https://ar.shein.com/" target="_blank" class="shein-img">
                            <img src="shein2egypt/static/img/sheinlogo.jpg"/>
                        </a>
                    </div>
                </div>

            </form>


        </t>
    </template>

